<table class="table table-striped table-condensed">
 <% @lists.each do |list| %>
   <tr>
     <td><%= list.name %></td>
     <td><%= link_to 'Show', list_path(list) %></td>
     <td><%= link_to 'Edit', edit_list_path(list) %></td>
     <td><%= link_to 'Destroy', list_path(list), method: :delete, data:{ confirm: "確定刪除嗎?" } %></td>
   </tr>
 <% end %>
</table>

